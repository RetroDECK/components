{
  "xemu": {
    "source_url": "https://github.com/xemu-project/xemu/releases/download/{VERSION}/*-x86_64.AppImage",
    "source_type": "github-release",
    "version": "$XEMU_DESIRED_VERSION",
    "extraction_type": "appimage",
    "assets": [
      {
        "type": "dir",
        "source": "usr/bin",
        "dest": "bin"
      }
    ],
    "libs": [
      {
        "library": "libslirp.so.0",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libpcap.so.0.8",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libcurl-gnutls.so.4",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libkrb5.so.3",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libgssapi_krb5.so.2",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libkrb5support.so.0",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "librtmp.so.1",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libssh.so.4",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libk5crypto.so.3",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "liblber-2.5.so.0",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libsasl2.so.2",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libkeyutils.so.1",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libldap-2.5.so.0",
        "source": "usr/lib",
        "dest": "lib"
      }
    ],
    "extras": [
      {
        "type": "dir",
        "source": "$REPO_ROOT/$COMPONENT_NAME",
        "dest": "$COMPONENT_ARTIFACT_ROOT"
      }
    ]
  }
}