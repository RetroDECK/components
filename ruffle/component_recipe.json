{
  "ruffle": [
    {
      "source_url": "rs.ruffle.Ruffle",
      "source_type": "flatpak_id",
      "version": "latest",
      "dest": "user",
      "extraction_type": "flatpak",
      "assets": [
        {
          "type": "dir",
          "source": "bin",
          "dest": "bin"
        }
      ],
      "libs": [],
      "extras": [
        {
          "type": "create",
          "dest": "component_version",
          "contents": "$SOURCE_VERSION"
        }
      ]
    }
  ]
}
