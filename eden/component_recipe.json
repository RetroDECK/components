{
  "eden": [
    {
      "source_url": "https://github.com/eden-emulator/Releases/releases/{VERSION}/*-amd64.AppImage",
      "source_type": "github-release",
      "version": "$EDEN_DESIRED_VERSION",
      "extraction_type": "appimage",
      "assets": [
        {
          "type": "dir",
          "source": "usr/bin",
          "dest": "bin"
        }
      ],
      "libs": [],
      "extras": [
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        }
      ]
    }
  ]
}