{
  "cemu": [
    {
      "source_url": "info.cemu.Cemu",
      "source_type": "flatpak_id",
      "version": "$CEMU_DESIRED_VERSION",
      "dest": "user",
      "extraction_type": "flatpak",
      "assets": [
        {
          "type": "dir",
          "source": "usr/bin",
          "dest": "bin"
        }
      ],
      "libs": [
        {
          "library": "libGLU.so.1",
          "source": "usr/lib",
          "dest": "lib"
        }
      ],
      "extras": [
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        }
      ]
    }
  ]
}