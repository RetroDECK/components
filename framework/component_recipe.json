{
    "framework": [
        {
            "source_url": "$REPO_ROOT/$COMPONENT_NAME",
            "source_type": "local",
            "extraction_type": "local",
            "version": "$FRAMEWORK_DESIRED_VERSION",
            "assets": [
                {
                    "type": "create",
                    "dest": "component_version",
                    "contents": "$FRAMEWORK_DESIRED_VERSION"
                },
                {
                    "type": "file",
                    "source": "component_functions.sh",
                    "dest": "$COMPONENT_ARTIFACT_ROOT"
                },
                {
                    "type": "file",
                    "source": "component_launcher.sh",
                    "dest": "$COMPONENT_ARTIFACT_ROOT"
                },
                {
                    "type": "file",
                    "source": "component_manifest.json",
                    "dest": "$COMPONENT_ARTIFACT_ROOT"
                },
                {
                    "type": "file",
                    "source": "component_prepare.sh",
                    "dest": "$COMPONENT_ARTIFACT_ROOT"
                },
                {
                    "type": "file",
                    "source": "component_recipe.json",
                    "dest": "$COMPONENT_ARTIFACT_ROOT"
                }
            ]
        }
    ]
}