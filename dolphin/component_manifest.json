{
  "dolphin": {
    "name": "Dolphin",
    "description": "Wii and GameCube Emulator",
    "exec": "bin/dolphin-emu",
    "args": "",
    "framework": ["qt6"],
    "run_extras": true,
    "url_rdwiki": "https://retrodeck.readthedocs.io/en/latest/wiki_emulator_guides/dolphin-primehack/dolphin-primehack-guide/",
    "url_webpage": "https://dolphin-emu.org",
    "url_source": "https://github.com/dolphin-emu/dolphin",
    "system": [
      "gc",
      "wii"
    ],
    "system_friendly_name":  [
      "GameCube",
      "Wii"
    ],
    "compatible_presets": {
      "abxy_button": [ "false", "true" ],
      "ask_to_exit": [ "false", "true" ],
      "cheevos": [ "false", "true" ],
      "cheevos_hardcore": [ "false", "true" ],
      "universal_dyn_input": [ "false", "true" ]
    },
    "preset_actions": {
      "config_file_format": "dolphin",
      "ask_to_exit": {
        "ConfirmStop": {
          "action": "change",
          "new_setting_value": "True",
          "section": "Interface",
          "target_file": "$dolphin_config",
          "defaults_file": "$config/Dolphin/Dolphin.ini"
        }
      },
      "cheevos": {
        "Enabled": {
          "action": "change",
          "new_setting_value": "True",
          "section": "Achievements",
          "target_file": "$dolphin_config_RetroAchievements",
          "defaults_file": "$config/Dolphin/RetroAchievements.ini"
        },
        "Username": {
          "action": "change",
          "new_setting_value": "$cheevos_username",
          "section": "Achievements",
          "target_file": "$dolphin_config_RetroAchievements",
          "defaults_file": "$config/Dolphin/RetroAchievements.ini"
        },
        "ApiToken": {
          "action": "change",
          "new_setting_value": "$cheevos_token",
          "section": "Achievements",
          "target_file": "$dolphin_config_RetroAchievements",
          "defaults_file": "$config/Dolphin/RetroAchievements.ini"
        }
      },
      "cheevos_hardcore": {
        "HardcoreEnabled": {
          "action": "change",
          "new_setting_value": "True",
          "section": "Achievements",
          "target_file": "$dolphin_config_RetroAchievements",
          "defaults_file": "$config/Dolphin/RetroAchievements.ini"
        }
      },
      "universal_dyn_input": {
        "#MOREINFONEEDED": {
        }
      },
      "abxy_button_swap": {
        "#MOREINFONEEDED": "value"
      }
    }
  }
}

