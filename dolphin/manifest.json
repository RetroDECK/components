{
    "dolphin": {
      "name": "Dolphin",
      "description": "Wii and GameCube Emulator",
      "url": "https://retrodeck.readthedocs.io/en/latest/wiki_emulator_guides/dolphin-primehack/dolphin-primehack-guide/",
      "cli-arg": "--version",
      "logs": "$XDG_DATA_HOME/dolphin-emu/dolphin.log",
      "system": [
        "gc",
        "wii"
      ],
      "properties": [
        {
          "abxy_button": true,
          "ask_to_exit": true,
          "cheevos": true,
          "cheevos_hardcore": true,
          "universal_dyn_input": true
        }
      ]
    }
}
