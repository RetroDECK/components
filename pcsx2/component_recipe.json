{
  "pcsx2": [
    {
      "source_url": "https://github.com/PCSX2/pcsx2/releases/download/{VERSION}/pcsx2-{VERSION}-linux-appimage-x64-Qt.AppImage",
      "source_type": "github_release",
      "version": "$PCSX2_DESIRED_VERSION",
      "extraction_type": "appimage",
      "assets": [
        {
          "type": "dir",
          "source": "usr/bin",
          "dest": "bin"
        },
        {
          "type": "dir",
          "source": "usr/plugins",
          "dest": "plugins"
        },
        {
          "type": "dir",
          "source": "apprun-hooks",
          "dest": "apprun-hooks"
        },
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "create",
          "dest": "component_version",
          "contents": "$SOURCE_VERSION"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_functions.sh",
           "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_launcher.sh",
           "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_manifest.json",
           "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_prepare.sh",
           "dest": "$COMPONENT_ARTIFACT_ROOT"
        }
      ],
      "libs": [
        {
          "library": "libQt6Widgets.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "6.10",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Gui.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "6.10",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Core.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "6.10",
          "dest": "shared-libs"
        },
        {
          "library": "libkddockwidgets-qt6.so.2.2",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libavformat.so.58",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libavcodec.so.58",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libavutil.so.56",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libbz2.so.1.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libgme.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libopenmpt.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libchromaprint.so.1",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libbluray.so.2",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "librabbitmq.so.4",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libsrt-gnutls.so.1.4",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libssh-gcrypt.so.4",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libzmq.so.5",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libswscale.so.5",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libswresample.so.3",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libsoxr.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libplutosvg.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libplutovg.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libpcap.so.0.8",
          "source": "usr/lib",
          "dest": "shared-libs"
        }
      ]
    }
  ]
}
