{
  "duckstation": [
    {
      "source_url": "https://github.com/RetroDECK/Duckstation/releases/download/{VERSION}/DuckStation-x64.AppImage",
      "source_type": "github_release",
      "version": "$DUCKSTATION_DESIRED_VERSION",
      "extraction_type": "appimage",
      "assets": [
        {
          "type": "dir",
          "source": "usr/bin",
          "dest": "bin"
        },
        {
          "type": "dir",
          "source": "apprun-hooks",
          "dest": "apprun-hooks"
        },
        {
          "type": "dir",
          "source": "usr/share/doc/",
          "dest": "doc"
        },
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "create",
          "dest": "component_version",
          "contents": "$SOURCE_VERSION"
        },
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME/rd_config",
          "dest": "rd_config"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_functions.sh",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_launcher.sh",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_manifest.json",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_recipe.json",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "$REPO_ROOT/$COMPONENT_NAME/component_prepare.sh",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        }
      ],
      "libs": [
        {
          "library": "libQt6Widgets.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Gui.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Core.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Multimedia.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Network.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6DBus.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6XcbQpa.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "libQt6Concurrent.so.6",
          "runtime_name": "org.kde.Platform",
          "runtime_version": "$DESIRED_QT6_RUNTIME_VERSION",
          "dest": "shared-libs"
        },
        {
          "library": "liblunasvg.so",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libsoundtouch.so.2",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libcpuinfo.so",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libcurl.so.4",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "librtmp.so.1",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libssh.so.4",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libldap-2.5.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "liblber-2.5.so.0",
          "source": "usr/lib",
          "dest": "shared-libs"
        },
        {
          "library": "libsasl2.so.2",
          "source": "usr/lib",
          "dest": "shared-libs"
        }
      ]
    }
  ]
}