{
  "xroar": [
    {
      "source_url": "https://github.com/RetroDECK/XRoar/releases/download/{VERSION}/xroar.tar.gz",
      "source_type": "github-release",
      "version": "latest",
      "extraction_type": "archive",
      "assets": [
        {
          "type": "dir",
          "source": "usr/bin",
          "dest": "bin"
        }
      ],
      "libs": [],
      "extras": [
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        }
      ]
    }
  ]
}