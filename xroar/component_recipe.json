{
  "xroar": [
    {
      "source_url": "https://github.com/RetroDECK/XRoar/releases/download/{VERSION}/xroar.tar.gz",
      "source_type": "github_release",
      "version": "latest",
      "extraction_type": "archive",
      "assets": [
        {
          "type": "dir",
          "source": "bin",
          "dest": "bin"
        }
      ],
      "libs": [],
      "extras": [
        {
          "type": "create",
          "dest": "component_version",
          "contents": "$SOURCE_VERSION"
        }
      ]
    }
  ]
}
