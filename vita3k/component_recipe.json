{
  "vita3k": [
    {
      "source_url": "https://github.com/Vita3K/Vita3K/releases/releases/download/{VERSION}/*.x86_64.AppImage",
      "source_type": "github_release",
      "version": "latest",
      "extraction_type": "appimage",
      "assets": [
        {
          "type": "dir",
          "source": "usr/bin",
          "dest": "bin"
        }
      ],
      "libs": [],
      "extras": [
        {
          "type": "dir",
          "source": "$REPO_ROOT/$COMPONENT_NAME",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "create",
          "dest": "component_version",
          "contents": "$SOURCE_VERSION"
        }
      ]
    }
  ]
}
