{
  "ryubing": [
    {
      "source_url": "io.github.ryubing.Ryujinx",
      "source_type": "flatpak_id",
      "version": "latest",
      "dest": "user",
      "extraction_type": "flatpak",
      "assets": [
        {
          "type": "dir",
          "source": "bin",
          "dest": "bin"
        },
        {
          "type": "create",
          "dest": "component_version",
          "contents": "$SOURCE_VERSION"
        }
      ],
      "libs": []
    }
  ]
}
