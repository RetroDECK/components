{
  "ryubing": {
    "name": "Ryubing",
    "description": "Ryubing is an open-source Nintendo Switch emulator",
    "url_rdwiki": "https://retrodeck.readthedocs.io/en/latest/wiki_emulator_guides/ryubing/ryubing-guide/",
    "url_source": "https://git.ryujinx.app/ryubing/ryujinx",
    "system_friendly_name": "Nintendo Switch",
    "system": "switch",
    "compatible_presets": {
      "abxy_button": [
        "false",
        "true"
      ],
      "ask_to_exit": [
        "false",
        "true"
      ]
    },
    "preset_actions": {
      "config_file_format": "ryubing",
      "ask_to_exit": [
        {
          "enabled_by_states": [ "true" ],
          "setting_name": "show_confirm_exit",
          "action": "change",
          "new_setting_value": "false",
          "section": "UI",
          "target_file": "$ryubing_config",
          "defaults_file": "$config/Ryubing/Config.json"
        }
      ],
      "abxy_button": [
        {
          "enabled_by_states": [ "true" ],
          "setting_name": "button_x",
          "action": "change",
          "new_setting_value": "Y",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$config/Ryubing/Config.json"
        },
        {
          "enabled_by_states": [ "true" ],
          "setting_name": "button_y",
          "action": "change",
          "new_setting_value": "X",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$config/Ryubing/Config.json"
        },
        {
          "enabled_by_states": [ "true" ],
          "setting_name": "button_b",
          "action": "change",
          "new_setting_value": "A",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$config/Ryubing/Config.json"
        },
        {
          "enabled_by_states": [ "true" ],
          "setting_name": "button_a",
          "action": "change",
          "new_setting_value": "B",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$config/Ryubing/Config.json"
        }
      ]
    }
  }
}
