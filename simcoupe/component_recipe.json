{
  "simcoupe": {
    "source_url": "https://github.com/simonowen/simcoupe/releases/download/{VERSION}/simcoupe_*_amd64.tar.gz",
    "source_type": "github-release",
    "version": "$SIMCOUPE_DESIRED_VERSION",
    "extraction_type": "archive",
    "assets": [
      {
        "type": "dir",
        "source": "usr/bin",
        "dest": "bin"
      }
    ],
    "libs": [],
    "extras": [
      {
        "type": "dir",
        "source": "$REPO_ROOT/$COMPONENT_NAME",
        "dest": "$COMPONENT_ARTIFACT_ROOT"
      }
    ]
  }
}
