{
  "primehack": {
    "name": "PrimeHack",
    "description": "A fork of Dolphin to enhance Metroid Prime experience",
    "url_rdwiki": "https://retrodeck.readthedocs.io/en/latest/wiki_emulator_guides/dolphin-primehack/dolphin-primehack-guide/",
    "url_webpage": "https://github.com/shiiion/dolphin/wiki/",
    "url_source": "https://github.com/shiiion/dolphin",
    "cli-arg": "--version",
    "logs": "$XDG_DATA_HOME/primehack/Logs/dolphin.log",
    "system": "wii",
    "compatible_presets": {
      "abxy_button": [
        "false",
        "true"
      ],
      "ask_to_exit": [
        "false",
        "true"
      ],
      "cheevos": [
        "false",
        "true"
      ],
      "cheevos_hardcore": [
        "false",
        "true"
      ],
      "universal_dyn_input": [
        "false",
        "true"
      ]
    },
    "preset_actions": {
      "config_file_format": "dolphin",
      "ask_to_exit": {
        "ConfirmStop": {
          "action": "change",
          "new_setting_value": "True",
          "section": "Interface",
          "target_file": "$primehack_config",
          "defaults_file": "$config/Primehack/Dolphin.ini"
        }
      },
      "cheevos": {
        "Enabled": {
          "action": "change",
          "new_setting_value": "True",
          "section": "Achievements",
          "target_file": "$primehack_config_RetroAchievements",
          "defaults_file": "$config/Primehack/RetroAchievements.ini"
        },
        "Username": {
          "action": "change",
          "new_setting_value": "$cheevos_username",
          "section": "Achievements",
          "target_file": "$primehack_config_RetroAchievements",
          "defaults_file": "$config/Primehack/RetroAchievements.ini"
        },
        "ApiToken": {
          "action": "change",
          "new_setting_value": "$cheevos_token",
          "section": "Achievements",
          "target_file": "$primehack_config_RetroAchievements",
          "defaults_file": "$config/Primehack/RetroAchievements.ini"
        }
      },
      "cheevos_hardcore": {
        "HardcoreEnabled": {
          "action": "change",
          "new_setting_value": "True",
          "section": "Achievements",
          "target_file": "$primehack_config_RetroAchievements",
          "defaults_file": "$config/Primehack/RetroAchievements.ini"
        }
      },
      "universal_dyn_input": {
        "#MOREINFONEEDED": {}
      },
      "abxy_button_swap": {
        "#MOREINFONEEDED": "value"
      }
    }
  }
}