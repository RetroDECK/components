{
  "openbor": {
    "source_url": "https://github.com/DCurrent/openbor/releases/download/{VERSION}/*.AppImage",
    "source_type": "github-release",
    "version": "$OPENBOR_DESIRED_VERSION",
    "extraction_type": "appimage",
    "assets": [
      {
        "type": "dir",
        "source": "usr/bin",
        "dest": "bin"
      }
    ],
    "libs": [
      {
        "library": "libSDL2_gfx-1.0.so.0",
        "source": "usr/lib",
        "dest": "lib"
      },
      {
        "library": "libvpx.so.5",
        "source": "usr/lib",
        "dest": "lib"
      }
    ],
    "extras": [
      {
        "type": "dir",
        "source": "$REPO_ROOT/$COMPONENT_NAME",
        "dest": "$COMPONENT_ARTIFACT_ROOT"
      }
    ]
  }
}
