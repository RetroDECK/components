{
  "ryubing": {
    "name": "Ryubing",
    "description": "Ryubing is a Nintendo Switch (switch) emulator.",
    "url_rdwiki": "https://retrodeck.readthedocs.io/en/latest/wiki_emulator_guides/ryubing/ryubing-guide/",
    "url_donation_purchase": "https://ryujinx.app/donate",
    "url_source": "https://git.ryujinx.app/ryubing/ryujinx",
    "component_type": "Emulator",
    "system_friendly_name": "Nintendo Switch",
    "system": "switch",
    "bios": [
      {
        "filename": "prod.keys",
        "system": "switch",
        "description": "Production keys for Switch emulation",
        "required": "Required",
        "paths": "$bios_path/switch/keys"
      },
      {
        "filename": "title.keys",
        "system": "switch",
        "description": "Title-specific keys for Switch emulation",
        "required": "Required",
        "paths": "$bios_path/switch/keys"
      }
    ],
    "compatible_presets": {
      "abxy_button_swap": [
        "false",
        "true"
      ],
      "ask_to_exit": [
        "false",
        "true"
      ]
    },
    "preset_actions": {
      "config_file_format": "ryubing",
      "ask_to_exit": [
        {
          "enabled_states": [
            "true"
          ],
          "setting_name": "show_confirm_exit",
          "action": "change",
          "new_setting_value": "false",
          "section": "UI",
          "target_file": "$ryubing_config",
          "defaults_file": "$ryubing_rd_config_dir/Config.json"
        }
      ],
      "abxy_button_swap": [
        {
          "enabled_states": [
            "true"
          ],
          "setting_name": "button_x",
          "action": "change",
          "new_setting_value": "Y",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$ryubing_rd_config_dir/Config.json"
        },
        {
          "enabled_states": [
            "true"
          ],
          "setting_name": "button_y",
          "action": "change",
          "new_setting_value": "X",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$ryubing_rd_config_dir/Config.json"
        },
        {
          "enabled_states": [
            "true"
          ],
          "setting_name": "button_b",
          "action": "change",
          "new_setting_value": "A",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$ryubing_rd_config_dir/Config.json"
        },
        {
          "enabled_states": [
            "true"
          ],
          "setting_name": "button_a",
          "action": "change",
          "new_setting_value": "B",
          "section": "right_joycon",
          "target_file": "$ryubing_config",
          "defaults_file": "$ryubing_rd_config_dir/Config.json"
        }
      ]
    }
  }
}