          "source": "demo.sym",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "diag.sym",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
        },
        {
          "type": "file",
          "source": "dos.sym",
          "dest": "$COMPONENT_ARTIFACT_ROOT"
